{
  "services": [
    {
      "id": "facebook",
      "name": "Facebook SDK",
      "description": "Facebook Login, Share, and Analytics",
      "icon": "üìò",
      "fields": [
        {
          "id": "appId",
          "label": "App ID",
          "placeholder": "Your Facebook App ID (e.g., 1234567890)",
          "required": true
        },
        {
          "id": "clientToken",
          "label": "Client Token",
          "placeholder": "Your Facebook Client Token",
          "required": true
        },
        {
          "id": "displayName",
          "label": "Display Name",
          "placeholder": "Your App Display Name",
          "required": true
        }
      ],
      "ios": {
        "plistEntries": [
          {
            "key": "FacebookAppID",
            "valueField": "appId",
            "type": "string"
          },
          {
            "key": "FacebookClientToken",
            "valueField": "clientToken",
            "type": "string"
          },
          {
            "key": "FacebookDisplayName",
            "valueField": "displayName",
            "type": "string"
          },
          {
            "key": "LSApplicationQueriesSchemes",
            "type": "array",
            "staticValue": ["fbapi", "fb-messenger-share-api", "fbauth2", "fbshareextension"]
          }
        ],
        "urlSchemes": [
          {
            "prefix": "fb",
            "valueField": "appId"
          }
        ],
        "appDelegate": {
          "import": "FBSDKCoreKit",
          "code": "ApplicationDelegate.shared.application(application, didFinishLaunchingWithOptions: launchOptions)"
        }
      },
      "android": {
        "metaData": [
          {
            "name": "com.facebook.sdk.ApplicationId",
            "valueField": "appId",
            "stringResource": "facebook_app_id"
          },
          {
            "name": "com.facebook.sdk.ClientToken",
            "valueField": "clientToken",
            "stringResource": "facebook_client_token"
          }
        ],
        "stringResources": [
          {
            "name": "facebook_app_id",
            "valueField": "appId"
          },
          {
            "name": "facebook_client_token",
            "valueField": "clientToken"
          },
          {
            "name": "fb_login_protocol_scheme",
            "valueField": "appId",
            "prefix": "fb"
          }
        ],
        "queries": [
          {
            "tag": "provider",
            "attributes": {
              "android:authorities": "com.facebook.katana.provider.PlatformProvider"
            }
          }
        ],
        "applicationData": [
          {
            "tag": "activity",
            "attributes": {
              "android:name": "com.facebook.FacebookActivity",
              "android:configChanges": "keyboard|keyboardHidden|screenLayout|screenSize|orientation",
              "android:label": "@string/app_name"
            }
          },
          {
            "tag": "activity",
            "attributes": {
              "android:name": "com.facebook.CustomTabActivity",
              "android:exported": "true"
            },
            "children": [
              {
                "tag": "intent-filter",
                "children": [
                  { "tag": "action", "attributes": { "android:name": "android.intent.action.VIEW" } },
                  { "tag": "category", "attributes": { "android:name": "android.intent.category.DEFAULT" } },
                  { "tag": "category", "attributes": { "android:name": "android.intent.category.BROWSABLE" } },
                  { "tag": "data", "attributes": { "android:scheme": "@string/fb_login_protocol_scheme" } }
                ]
              }
            ]
          }
        ]
      }
    },
